CREATE TABLE usuarios (
                username TEXT PRIMARY KEY,
                email TEXT,
                full_name TEXT,
                age INTEGER,
                role TEXT,
                is_active INTEGER,
                last_login TEXT,
                avatar_path TEXT
            )
;
SELECT * FROM usuarios